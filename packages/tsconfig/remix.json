{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./rules.json",
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ES2019"],
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "target": "ES2019",
    "paths": {
      "~/*": ["./app/*"]
    },
    "noEmit": true
  }
}
