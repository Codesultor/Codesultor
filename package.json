{
  "name": "codesultor",
  "private": true,
  "scripts": {
    "dev": "pnpm -r dev",
    "test": "pnpm test:eslint && pnpm test:prettier && pnpm test:types",
    "test:eslint": "pnpm -r test:eslint",
    "test:prettier": "prettier --check .",
    "test:types": "pnpm -r test:types",
    "format": "pnpm format:eslint && pnpm format:prettier",
    "format:eslint": "pnpm run -r format:eslint",
    "format:prettier": "prettier --write ."
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/prettier": "^2.7.2",
    "prettier": "^2.8.8",
    "prettier-plugin-jsdoc": "^0.4.2"
  },
  "packageManager": "pnpm@8.3.1"
}
